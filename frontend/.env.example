# 应用配置
VITE_APP_TITLE=安全API系统
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=企业级安全管理平台

# API 配置
VITE_API_BASE_URL=http://localhost:8080/api
VITE_API_TIMEOUT=30000
VITE_API_RETRY_COUNT=3

# 认证配置
VITE_JWT_SECRET_KEY=your-jwt-secret-key
VITE_JWT_EXPIRES_IN=7d
VITE_REFRESH_TOKEN_EXPIRES_IN=30d

# 加密配置
VITE_AES_SECRET_KEY=your-aes-secret-key
VITE_RSA_PUBLIC_KEY=your-rsa-public-key
VITE_RSA_PRIVATE_KEY=your-rsa-private-key

# 上传配置
VITE_UPLOAD_MAX_SIZE=10485760
VITE_UPLOAD_ALLOWED_TYPES=image/jpeg,image/png,image/gif,application/pdf,text/plain

# 分页配置
VITE_DEFAULT_PAGE_SIZE=20
VITE_MAX_PAGE_SIZE=100

# 缓存配置
VITE_CACHE_EXPIRES_IN=3600000
VITE_LOCAL_STORAGE_PREFIX=secure_api_

# 日志配置
VITE_LOG_LEVEL=info
VITE_LOG_MAX_FILES=10
VITE_LOG_MAX_SIZE=10485760

# 安全配置
VITE_ENABLE_CAPTCHA=true
VITE_CAPTCHA_LENGTH=4
VITE_MAX_LOGIN_ATTEMPTS=5
VITE_ACCOUNT_LOCK_DURATION=300000

# 功能开关
VITE_ENABLE_REGISTRATION=true
VITE_ENABLE_FORGOT_PASSWORD=true
VITE_ENABLE_TWO_FACTOR_AUTH=true
VITE_ENABLE_EMAIL_VERIFICATION=true
VITE_ENABLE_PHONE_VERIFICATION=true

# 第三方服务
VITE_GOOGLE_ANALYTICS_ID=
VITE_SENTRY_DSN=
VITE_HOTJAR_ID=

# 邮件服务
VITE_EMAIL_SERVICE_URL=
VITE_EMAIL_FROM=noreply@example.com

# 短信服务
VITE_SMS_SERVICE_URL=
VITE_SMS_APP_KEY=
VITE_SMS_APP_SECRET=

# 文件存储
VITE_FILE_STORAGE_TYPE=local
VITE_FILE_STORAGE_URL=
VITE_FILE_STORAGE_BUCKET=

# 监控配置
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_ENABLE_ERROR_TRACKING=true
VITE_ENABLE_USER_TRACKING=false

# 开发配置
VITE_ENABLE_MOCK=false
VITE_ENABLE_DEVTOOLS=true
VITE_ENABLE_HOT_RELOAD=true

# 生产配置
VITE_ENABLE_COMPRESSION=true
VITE_ENABLE_BUNDLE_ANALYZER=false
VITE_ENABLE_SOURCE_MAP=false

# CDN 配置
VITE_CDN_URL=
VITE_STATIC_URL=

# 国际化配置
VITE_DEFAULT_LOCALE=zh-CN
VITE_SUPPORTED_LOCALES=zh-CN,en-US

# 主题配置
VITE_DEFAULT_THEME=light
VITE_ENABLE_DARK_MODE=true

# 网络配置
VITE_NETWORK_TIMEOUT=10000
VITE_NETWORK_RETRY_DELAY=1000

# WebSocket 配置
VITE_WS_URL=ws://localhost:8080/ws
VITE_WS_RECONNECT_INTERVAL=5000
VITE_WS_MAX_RECONNECT_ATTEMPTS=10

# 推送通知配置
VITE_ENABLE_PUSH_NOTIFICATIONS=true
VITE_VAPID_PUBLIC_KEY=

# 地图服务配置
VITE_MAP_API_KEY=
VITE_MAP_SERVICE_URL=

# 支付配置
VITE_PAYMENT_GATEWAY_URL=
VITE_PAYMENT_PUBLIC_KEY=

# 社交登录配置
VITE_OAUTH_GOOGLE_CLIENT_ID=
VITE_OAUTH_GITHUB_CLIENT_ID=
VITE_OAUTH_WECHAT_APP_ID=

# 数据库配置（仅开发环境）
VITE_DB_HOST=localhost
VITE_DB_PORT=3306
VITE_DB_NAME=secure_api_system
VITE_DB_USER=root
VITE_DB_PASSWORD=

# Redis 配置（仅开发环境）
VITE_REDIS_HOST=localhost
VITE_REDIS_PORT=6379
VITE_REDIS_PASSWORD=
VITE_REDIS_DB=0

# 备份配置
VITE_BACKUP_ENABLED=true
VITE_BACKUP_INTERVAL=86400000
VITE_BACKUP_RETENTION_DAYS=30

# 审计配置
VITE_AUDIT_ENABLED=true
VITE_AUDIT_LOG_LEVEL=info
VITE_AUDIT_RETENTION_DAYS=90